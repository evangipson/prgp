;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="PRPG"
run/main_scene="res://Screens/root.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

ColorService="*res://Singletons/ColorService.cs"
ScreenService="*res://Singletons/ScreenService.cs"
PlayerService="*res://Singletons/PlayerService.cs"
CodeEditorFactory="*res://Singletons/CodeEditorFactory.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1200
window/size/mode=3
window/size/resizable=false
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="PRPG"
